#include<stdio.h>
//Gobal Variable
int size;
int* getNewArray(){
    printf("enter size of array:\n");
    scanf("%d",&size);
    static int arr[100];
    printf("enter %d elements of array:\n",size);
    for(int i=0;i<=size-1;i++){
    scanf("%d",&arr[i]);
    }
    return arr;
}

int* createCopyArray(int *p){
 static int newarr[100];
 for(int i=0;i<=size-1;i++){
     newarr[i]=p[i];
 }
 return newarr;
}
int main(){
    int *array=getNewArray();
    int *coppyArray=createCopyArray(array);
    for(int i=0;i<=size-1;i++){
        printf("%d",coppyArray[i]);
    }
    return 0;
}
